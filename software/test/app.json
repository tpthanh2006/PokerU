    },
    "plugins": [
      "expo-router",
      [
        "@clerk/clerk-expo",
        {
          "publishableKey": "pk_test_YnJhdmUtbGFyay0yNS5jbGVyay5hY2NvdW50cy5kZXYk"
        }
      ],
      [
      "expo-splash-screen",
      {
        "backgroundColor": "#1a0325",
